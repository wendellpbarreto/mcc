        <h2>Artworks</h2>
        <ol class="breadcrumb">
            <li><%= link_to t('Artworks'), artworks_path %></li>
            <li class="active"><strong><%= t('Create') %></strong></li>
        </ol>
    </div>
    <div class="col-sm-12 col-md-2">

    </div>
</div>

